programa
{
    funcao inicio()
    {
        // Declaração de variável
        inteiro i

        // Laço de repetição para a contagem regressiva de 10 a 1
        para (i = 100; i >= 1; i--)
        {
            escreva(i, "\n")
        }

        // Exibe uma mensagem ao final da contagem
        escreva("Contagem regressiva finalizada!\n")
    }
}
